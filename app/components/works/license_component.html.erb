<section>
  <header>Terms of use and license</header>
  <div class="mb-3 row">
    <%= form.label :license, class: 'col-sm-2 col-form-label' %>
    <div class="col-sm-10">
      <%= form.select :license,
                      License.all.collect {|p| [ p.label, p.id ] },
                      {}, class: "form-select" %>
    </div>
  </div>
</section>
